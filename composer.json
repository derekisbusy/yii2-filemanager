{
    "name": "derekisbusy/yii2-filemanager",
    "description": "Yii2 filemanager",
    "type": "yii2-extension",
    "license": "GNU GPLv2",
    "authors": [
        {
            "name": "PendalF",
            "email": "pendalf@mail.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-imagine": "*",
        "pendalf89/yii2-tinymce": "*",
        "kartik-v/yii2-widget-alert": "*",
        "2amigos/yii2-file-upload-widget": "*"
    },
    "autoload": {
        "psr-4": {
            "pendalf89\\filemanager\\": ""
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "derekisbusy/yii2-filemanager",
                "version": "dev-master",
                "source": {
                    "url": "git://github.com/derekisbusy/yii2-filemanager.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0" : {
                        "derekisbusy\\filemanager" : "src"
                    }
                }
            }
        }
    ],
}
